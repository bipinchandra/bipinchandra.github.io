
<% if(data.company.team.tagline != "nil") %>
<div class="row"><div class="col-md-12"><%= data.company.team.tagline %></div></div>
<% end %>

<br>

<div class="row">

  <% for member_id in data.company.team.members.keys %>
  <%
     name = data.company.team.members[member_id].name
     profie = data.company.team.members[member_id].profile
     image =  data.company.team.members[member_id].image
     text =  data.company.team.members[member_id].text
  %>
  <div class="col-md-8">
    <div class="row">
      <div class="col-md-12"><b><%= name %></b></div>
      <div class="col-md-12"><%= text %></div>
    </div>
    <br>
  </div>
  <div class="col-md-4">
    <img class="img-responsive" src="<%= image %>">
  </div>
  <% end %>
 
</div>
